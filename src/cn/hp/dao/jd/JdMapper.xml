<mapper namespace="cn.hp.dao.jd.JdMapper">
	<!-- 查询设备信息 -->
	<select id="count" resultType="int">
		SELECT count(1) FROM `ch1jd4`
	</select>
	<select id="ListAll" resultType="Jd">
		SELECT * FROM `ch1jd4` WHERE `date` > '2015-01-01'
	</select>
